<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="#CABBBBBB" />

            <corners android:radius="7dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="1dp"
        android:right="4dp"
        android:top="0dp">
        <shape android:shape="rectangle" >
            <solid android:color="@color/session_background_color" />

            <corners android:radius="7dp" />
        </shape>
    </item>

    <item android:top="10dp" android:right="10dp">
      <shape 
        android:shape="rectangle">
            <stroke android:width="4dp" android:color="#CABBBBBB" />
            <corners android:radius="2dp" />
            <solid android:color="@color/session_background_color" />

        </shape>
   </item>
   
   <item android:bottom="4dp" android:right="9dp" android:top="9dp"> 
      <shape 
        android:shape="rectangle">
            <corners android:radius="2dp" />
            <solid android:color="@color/session_background_color" />
        </shape>
   </item>
    
   
   
    
    <item
        android:bottom="4dp"
        android:top="10dp"
        android:right="10dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="#CABBBBBB" />

            <solid android:color="@color/session_background_color" />
        </shape>
    </item>

</layer-list>